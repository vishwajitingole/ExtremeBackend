<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dikhao</title>
  </head>

  <body>
    <h1>Photo</h1>
    <% files.forEach(file => { %>
    <div style="display: flex; flex-direction: horizontal" class="">
      <img
        style="width: 200px"
        src="data:image/jpeg;base64,<%= file.image.toString('base64') %>"
        alt="Uploaded Image"
      />
    </div>
    <% }); %>
  </body>
</html>
